<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.neonae">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logoicone"
        android:label="Neon AE"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LogarActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MenuActivity" />
        <activity android:name=".CadastrarActivity" />
        <activity android:name=".PerfilActivity" />
        <activity android:name=".EventosActivity" />
        <activity android:name=".InfoEvenActivity"></activity>
    </application>

</manifest>